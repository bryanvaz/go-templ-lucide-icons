package icons

// Renders the Lucide icon anchor.
templ Anchor(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-anchor", attrs) }
	>
		<circle cx="12" cy="5" r="3"></circle>
		<line x1="12" x2="12" y1="22" y2="8"></line>
		<path d="M5 12H2a10 10 0 0 0 20 0h-3"></path>
		{ children... }
	</svg>
}
