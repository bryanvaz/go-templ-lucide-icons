package icons

// Renders the Lucide icon spline.
templ Spline(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-spline", attrs) }
	>
		<circle cx="19" cy="5" r="2"></circle>
		<circle cx="5" cy="19" r="2"></circle>
		<path d="M5 17A12 12 0 0 1 17 5"></path>
		{ children... }
	</svg>
}
