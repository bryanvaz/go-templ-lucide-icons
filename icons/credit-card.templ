package icons

// Renders the Lucide icon credit-card.
templ CreditCard(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-credit-card", attrs) }
	>
		<rect width="20" height="14" x="2" y="5" rx="2"></rect>
		<line x1="2" x2="22" y1="10" y2="10"></line>
		{ children... }
	</svg>
}
