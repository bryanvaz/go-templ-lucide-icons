package icons

// Renders the Lucide icon pause-circle.
templ PauseCircle(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-pause-circle", attrs) }
	>
		<circle cx="12" cy="12" r="10"></circle>
		<line x1="10" x2="10" y1="15" y2="9"></line>
		<line x1="14" x2="14" y1="15" y2="9"></line>
		{ children... }
	</svg>
}
