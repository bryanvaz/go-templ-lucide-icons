package icons

// Renders the Lucide icon album.
templ Album(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-album", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2" ry="2"></rect>
		<polyline points="11 3 11 11 14 8 17 11 17 3"></polyline>
		{ children... }
	</svg>
}
