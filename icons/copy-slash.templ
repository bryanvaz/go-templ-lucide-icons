package icons

// Renders the Lucide icon copy-slash.
templ CopySlash(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-copy-slash", attrs) }
	>
		<line x1="12" x2="18" y1="18" y2="12"></line>
		<rect width="14" height="14" x="8" y="8" rx="2" ry="2"></rect>
		<path d="M4 16c-1.1 0-2-.9-2-2V4c0-1.1.9-2 2-2h10c1.1 0 2 .9 2 2"></path>
		{ children... }
	</svg>
}
