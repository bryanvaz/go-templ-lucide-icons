package icons

// Renders the Lucide icon align-justify.
templ AlignJustify(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-align-justify", attrs) }
	>
		<line x1="3" x2="21" y1="6" y2="6"></line>
		<line x1="3" x2="21" y1="12" y2="12"></line>
		<line x1="3" x2="21" y1="18" y2="18"></line>
		{ children... }
	</svg>
}
