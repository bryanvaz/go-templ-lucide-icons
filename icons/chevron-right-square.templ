package icons

// Renders the Lucide icon chevron-right-square.
templ ChevronRightSquare(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-chevron-right-square", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<path d="m10 8 4 4-4 4"></path>
		{ children... }
	</svg>
}
