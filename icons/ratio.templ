package icons

// Renders the Lucide icon ratio.
templ Ratio(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-ratio", attrs) }
	>
		<rect width="12" height="20" x="6" y="2" rx="2"></rect>
		<rect width="20" height="12" x="2" y="6" rx="2"></rect>
		{ children... }
	</svg>
}
