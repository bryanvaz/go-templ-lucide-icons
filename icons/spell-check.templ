package icons

// Renders the Lucide icon spell-check.
templ SpellCheck(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-spell-check", attrs) }
	>
		<path d="m6 16 6-12 6 12"></path>
		<path d="M8 12h8"></path>
		<path d="m16 20 2 2 4-4"></path>
		{ children... }
	</svg>
}
