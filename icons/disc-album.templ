package icons

// Renders the Lucide icon disc-album.
templ DiscAlbum(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-disc-album", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<circle cx="12" cy="12" r="5"></circle>
		<path d="M12 12h.01"></path>
		{ children... }
	</svg>
}
