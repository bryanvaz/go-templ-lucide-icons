package icons

// Renders the Lucide icon arrow-down-circle.
templ ArrowDownCircle(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-arrow-down-circle", attrs) }
	>
		<circle cx="12" cy="12" r="10"></circle>
		<path d="M12 8v8"></path>
		<path d="m8 12 4 4 4-4"></path>
		{ children... }
	</svg>
}
