package icons

// Renders the Lucide icon layout-list.
templ LayoutList(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-layout-list", attrs) }
	>
		<rect width="7" height="7" x="3" y="3" rx="1"></rect>
		<rect width="7" height="7" x="3" y="14" rx="1"></rect>
		<path d="M14 4h7"></path>
		<path d="M14 9h7"></path>
		<path d="M14 15h7"></path>
		<path d="M14 20h7"></path>
		{ children... }
	</svg>
}
