package icons

// Renders the Lucide icon captions.
templ Captions(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-captions", attrs) }
	>
		<rect width="18" height="14" x="3" y="5" rx="2" ry="2"></rect>
		<path d="M7 15h4M15 15h2M7 11h2M13 11h4"></path>
		{ children... }
	</svg>
}
