package icons

// Renders the Lucide icon pie-chart.
templ PieChart(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-pie-chart", attrs) }
	>
		<path d="M21.21 15.89A10 10 0 1 1 8 2.83"></path>
		<path d="M22 12A10 10 0 0 0 12 2v10z"></path>
		{ children... }
	</svg>
}
