package icons

// Renders the Lucide icon arrow-down-square.
templ ArrowDownSquare(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-arrow-down-square", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<path d="M12 8v8"></path>
		<path d="m8 12 4 4 4-4"></path>
		{ children... }
	</svg>
}
