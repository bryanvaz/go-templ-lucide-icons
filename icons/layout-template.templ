package icons

// Renders the Lucide icon layout-template.
templ LayoutTemplate(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-layout-template", attrs) }
	>
		<rect width="18" height="7" x="3" y="3" rx="1"></rect>
		<rect width="9" height="7" x="3" y="14" rx="1"></rect>
		<rect width="5" height="7" x="16" y="14" rx="1"></rect>
		{ children... }
	</svg>
}
