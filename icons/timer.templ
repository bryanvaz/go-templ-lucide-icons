package icons

// Renders the Lucide icon timer.
templ Timer(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-timer", attrs) }
	>
		<line x1="10" x2="14" y1="2" y2="2"></line>
		<line x1="12" x2="15" y1="14" y2="11"></line>
		<circle cx="12" cy="14" r="8"></circle>
		{ children... }
	</svg>
}
