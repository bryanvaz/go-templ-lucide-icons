package icons

// Renders the Lucide icon x-circle.
templ XCircle(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-x-circle", attrs) }
	>
		<circle cx="12" cy="12" r="10"></circle>
		<path d="m15 9-6 6"></path>
		<path d="m9 9 6 6"></path>
		{ children... }
	</svg>
}
