package icons

// Renders the Lucide icon app-window.
templ AppWindow(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-app-window", attrs) }
	>
		<rect x="2" y="4" width="20" height="16" rx="2"></rect>
		<path d="M10 4v4"></path>
		<path d="M2 8h20"></path>
		<path d="M6 4v4"></path>
		{ children... }
	</svg>
}
