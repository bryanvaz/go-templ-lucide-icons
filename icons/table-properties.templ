package icons

// Renders the Lucide icon table-properties.
templ TableProperties(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-table-properties", attrs) }
	>
		<path d="M15 3v18"></path>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<path d="M21 9H3"></path>
		<path d="M21 15H3"></path>
		{ children... }
	</svg>
}
