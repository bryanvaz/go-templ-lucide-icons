package icons

// Renders the Lucide icon activity-square.
templ ActivitySquare(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-activity-square", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<path d="M17 12h-2l-2 5-2-10-2 5H7"></path>
		{ children... }
	</svg>
}
