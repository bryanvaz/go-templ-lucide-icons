package icons

// Renders the Lucide icon compass.
templ Compass(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-compass", attrs) }
	>
		<circle cx="12" cy="12" r="10"></circle>
		<polygon points="16.24 7.76 14.12 14.12 7.76 16.24 9.88 9.88 16.24 7.76"></polygon>
		{ children... }
	</svg>
}
