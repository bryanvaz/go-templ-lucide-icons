package icons

// Renders the Lucide icon torus.
templ Torus(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-torus", attrs) }
	>
		<ellipse cx="12" cy="11" rx="3" ry="2"></ellipse>
		<ellipse cx="12" cy="12.5" rx="10" ry="8.5"></ellipse>
		{ children... }
	</svg>
}
