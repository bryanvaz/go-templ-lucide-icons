package icons

// Renders the Lucide icon columns-3.
templ Columns3(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-columns-3", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<path d="M9 3v18"></path>
		<path d="M15 3v18"></path>
		{ children... }
	</svg>
}
