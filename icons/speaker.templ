package icons

// Renders the Lucide icon speaker.
templ Speaker(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-speaker", attrs) }
	>
		<rect width="16" height="20" x="4" y="2" rx="2"></rect>
		<path d="M12 6h.01"></path>
		<circle cx="12" cy="14" r="4"></circle>
		<path d="M12 14h.01"></path>
		{ children... }
	</svg>
}
