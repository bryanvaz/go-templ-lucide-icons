package icons

// Renders the Lucide icon hash.
templ Hash(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-hash", attrs) }
	>
		<line x1="4" x2="20" y1="9" y2="9"></line>
		<line x1="4" x2="20" y1="15" y2="15"></line>
		<line x1="10" x2="8" y1="3" y2="21"></line>
		<line x1="16" x2="14" y1="3" y2="21"></line>
		{ children... }
	</svg>
}
