package icons

// Renders the Lucide icon circle-plus.
templ CirclePlus(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-circle-plus", attrs) }
	>
		<circle cx="12" cy="12" r="10"></circle>
		<path d="M8 12h8"></path>
		<path d="M12 8v8"></path>
		{ children... }
	</svg>
}
