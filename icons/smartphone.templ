package icons

// Renders the Lucide icon smartphone.
templ Smartphone(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-smartphone", attrs) }
	>
		<rect width="14" height="20" x="5" y="2" rx="2" ry="2"></rect>
		<path d="M12 18h.01"></path>
		{ children... }
	</svg>
}
