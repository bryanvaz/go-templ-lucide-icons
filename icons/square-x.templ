package icons

// Renders the Lucide icon square-x.
templ SquareX(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-square-x", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2" ry="2"></rect>
		<path d="m15 9-6 6"></path>
		<path d="m9 9 6 6"></path>
		{ children... }
	</svg>
}
