package icons

// Renders the Lucide icon toggle-left.
templ ToggleLeft(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-toggle-left", attrs) }
	>
		<rect width="20" height="12" x="2" y="6" rx="6" ry="6"></rect>
		<circle cx="8" cy="12" r="2"></circle>
		{ children... }
	</svg>
}
