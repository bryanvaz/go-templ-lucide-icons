package icons

// Renders the Lucide icon videotape.
templ Videotape(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-videotape", attrs) }
	>
		<rect width="20" height="16" x="2" y="4" rx="2"></rect>
		<path d="M2 8h20"></path>
		<circle cx="8" cy="14" r="2"></circle>
		<path d="M8 12h8"></path>
		<circle cx="16" cy="14" r="2"></circle>
		{ children... }
	</svg>
}
