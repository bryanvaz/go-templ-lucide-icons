package icons

// Renders the Lucide icon asterisk.
templ Asterisk(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-asterisk", attrs) }
	>
		<path d="M12 6v12"></path>
		<path d="M17.196 9 6.804 15"></path>
		<path d="m6.804 9 10.392 6"></path>
		{ children... }
	</svg>
}
