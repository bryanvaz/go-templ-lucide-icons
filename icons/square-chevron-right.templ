package icons

// Renders the Lucide icon square-chevron-right.
templ SquareChevronRight(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-square-chevron-right", attrs) }
	>
		<rect width="18" height="18" x="3" y="3" rx="2"></rect>
		<path d="m10 8 4 4-4 4"></path>
		{ children... }
	</svg>
}
