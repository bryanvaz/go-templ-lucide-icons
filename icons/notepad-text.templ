package icons

// Renders the Lucide icon notepad-text.
templ NotepadText(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-notepad-text", attrs) }
	>
		<path d="M8 2v4"></path>
		<path d="M12 2v4"></path>
		<path d="M16 2v4"></path>
		<rect width="16" height="18" x="4" y="4" rx="2"></rect>
		<path d="M8 10h6"></path>
		<path d="M8 14h8"></path>
		<path d="M8 18h5"></path>
		{ children... }
	</svg>
}
