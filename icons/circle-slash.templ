package icons

// Renders the Lucide icon circle-slash.
templ CircleSlash(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-circle-slash", attrs) }
	>
		<line x1="9" x2="15" y1="15" y2="9"></line>
		<circle cx="12" cy="12" r="10"></circle>
		{ children... }
	</svg>
}
