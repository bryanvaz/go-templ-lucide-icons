package icons

// Renders the Lucide icon italic.
templ Italic(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-italic", attrs) }
	>
		<line x1="19" x2="10" y1="4" y2="4"></line>
		<line x1="14" x2="5" y1="20" y2="20"></line>
		<line x1="15" x2="9" y1="4" y2="20"></line>
		{ children... }
	</svg>
}
