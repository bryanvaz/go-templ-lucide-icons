package icons

// Renders the Lucide icon text-cursor.
templ TextCursor(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-text-cursor", attrs) }
	>
		<path d="M17 22h-1a4 4 0 0 1-4-4V6a4 4 0 0 1 4-4h1"></path>
		<path d="M7 22h1a4 4 0 0 0 4-4v-1"></path>
		<path d="M7 2h1a4 4 0 0 1 4 4v1"></path>
		{ children... }
	</svg>
}
