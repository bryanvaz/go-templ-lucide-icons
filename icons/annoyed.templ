package icons

// Renders the Lucide icon annoyed.
templ Annoyed(attrs ...templ.Attributes) {
	<svg
		{ at(attrs)... }
		class={ cn("lucide lucide-annoyed", attrs) }
	>
		<circle cx="12" cy="12" r="10"></circle>
		<path d="M8 15h8"></path>
		<path d="M8 9h2"></path>
		<path d="M14 9h2"></path>
		{ children... }
	</svg>
}
